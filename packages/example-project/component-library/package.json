{
  "name": "component-library",
  "version": "0.0.4",
  "private": true,
  "description": "Stencil Component Library for testing purposes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-ds-output-targets.git"
  },
  "exports": {
    "./components": {
      "import": "./dist/components",
      "require": "./dist/components"
    }
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/interfaces.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/component-library/component-library.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.angular-library": "stencil build --docs --config stencil-angular.config.ts",
    "build.react-library": "stencil build --docs --config stencil-react.config.ts",
    "build.vue-library": "stencil build --docs --config stencil-vue.config.ts",
    "build.svelte-library": "stencil build --docs --config stencil-svelte.config.ts",
    "test": "echo \"No tests specified\" && exit 0",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.9.0",
    "@types/puppeteer": "2.0.1",
    "@types/jest": "^26.0.1",
    "jest": "^26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "2.1.1",
    "typescript": "^4.3"
  }
}
