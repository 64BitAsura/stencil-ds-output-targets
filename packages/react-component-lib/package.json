{
  "name": "@ionic-enterprise/react-component-lib",
  "version": "0.0.0",
  "description": "This is an output plugin for stencil.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "resources/"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist && npm run lint",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c",
    "version": "npm run build",
    "release": "np",
    "lint": "tslint --project tsconfig.json",
    "test": "jest --passWithNoTests",
    "test.watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-ds-plugins.git"
  },
  "author": "Ionic Team",
  "homepage": "https://stenciljs.com/",
  "license": "Commercial",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil-ds-plugins/issues"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.1",
    "@trust/webcrypto": "^0.9.2",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.7.0",
    "@types/react-dom": "^16.7.0",
    "jest": "^24.5.0",
    "np": "^5.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-testing-library": "^7.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "tslint": "^5.14.0",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/fixture-component-lib/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts?|tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  }
}
